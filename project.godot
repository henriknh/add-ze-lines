; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Operator",
"class": "End",
"language": "GDScript",
"path": "res://operators/end.gd"
}, {
"base": "Operator",
"class": "Hub",
"language": "GDScript",
"path": "res://operators/hub.gd"
}, {
"base": "Line2D",
"class": "InputChain",
"language": "GDScript",
"path": "res://operators/input_chain/input_chain.gd"
}, {
"base": "Node2D",
"class": "Operator",
"language": "GDScript",
"path": "res://operators/operator.gd"
}, {
"base": "Operator",
"class": "Start",
"language": "GDScript",
"path": "res://operators/start.gd"
} ]
_global_script_class_icons={
"End": "",
"Hub": "",
"InputChain": "",
"Operator": "",
"Start": ""
}

[application]

config/name="lines"
run/main_scene="res://menu/menu.tscn"
config/icon="res://icon.png"

[autoload]

Random="*res://singletons/random.gd"
Arithmetic="*res://singletons/arithmetic.gd"
Level="*res://singletons/level.gd"
UserInput="*res://singletons/user_input.gd"
Settings="*res://singletons/settings.gd"
Data="*res://singletons/data.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/width=384
window/size/height=832
window/vsync/use_vsync=false
window/handheld/orientation="portrait"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.0666667, 0.0588235, 0.243137, 1 )
quality/depth/hdr=false
environment/default_environment="res://default_env.tres"
